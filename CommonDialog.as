package  {	import flash.events.MouseEvent;	import flash.display.MovieClip;	import flash.events.Event;	public class CommonDialog extends MovieClip{		public function CommonDialog() {			// constructor code			//this.txtMessageLabel.text = "恭喜你，完全正确的分类饿了所有的垃圾！"						this.btnRight.addEventListener(MouseEvent.CLICK, onBtnRightTouched)						this.btnLeft.addEventListener(MouseEvent.CLICK, onBtnLeftTouched)					}		private function onBtnRightTouched(evt):void{			trace("onBtnRightTouched")			this.parent.removeChild(this)			Game.getInstance().dispatchEvent(new Event("START_GAME"))		}		private function onBtnLeftTouched(evt):void{			trace("onBtnLeftTouched")				this.parent.removeChild(this)			Game.getInstance().dispatchEvent(new Event("BACK_TO_HOME_PAGE_FROM_MAIN"))		}	}	}