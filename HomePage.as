package  {	import flash.display.MovieClip;	import flash.display.DisplayObject;	import flash.events.MouseEvent;	import flash.events.Event;	public class HomePage extends MovieClip{		public function HomePage() {			// constructor code			this.btnStart.addEventListener(MouseEvent.CLICK, onBtnStartTouched)			this.btnHelp.addEventListener(MouseEvent.CLICK, onBtnHelpTouched)		}		private function onBtnStartTouched(evt):void{			Game.getInstance().dispatchEvent(new Event("START_GAME"))		}		private function onBtnHelpTouched(evt):void{			Game.getInstance().dispatchEvent(new Event("START_HELP"))		}	}	}